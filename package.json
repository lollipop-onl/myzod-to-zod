{
	"name": "@lollipop-onl/myzod-to-zod",
	"version": "1.2.0",
	"description": "A codemod to migrate from myzod to zod v3",
	"homepage": "https://github.com/lollipop-onl/myzod-to-zod#readme",
	"repository": "lollipop-onl/myzod-to-zod",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"require": "./dist/index.cjs",
			"default": "./dist/index.mjs"
		}
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"bin": "./dist/cli.mjs",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "unbuild",
		"lint": "biome check .",
		"release": "semantic-release",
		"start": "tsx src/cli.ts",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"citty": "^0.1.6",
		"myzod": "^1.12.1",
		"prettier": "^3.6.2",
		"ts-morph": "^26.0.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.3",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/github": "^11.0.5",
		"@semantic-release/npm": "^12.0.2",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@tsconfig/node24": "^24.0.1",
		"@types/node": "^24.3.0",
		"semantic-release": "^24.2.7",
		"tsx": "^4.20.5",
		"typescript": "^5.6.3",
		"unbuild": "^3.6.1",
		"vitest": "^3.2.4"
	},
	"publishConfig": {
		"access": "public"
	}
}
